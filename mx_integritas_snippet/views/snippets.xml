<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="register_s_website_form" inherit_id="website.snippets" name="Snippet Form">
        <xpath expr="//div[@id='snippet_feature']//t[@t-snippet][last()]" position="after">
            <t t-snippet="mx_integritas_snippet.mx_integritas_form" t-thumbnail="/website/static/src/img/s_website_form.png"/>
        </xpath>
    </template>
    <template id="snippet_integritas_carousel" inherit_id="website.snippets" name="Snippet Carousel">
        <xpath expr="//div[@id='snippet_feature']//t[@t-snippet][last()]" position="after">
            <t t-snippet="mx_integritas_snippet.mx_integritas_carousel" t-thumbnail="/mx_integritas_snippet/static/src/img/integritas.png"/>
        </xpath>
    </template>
    
    
</odoo>