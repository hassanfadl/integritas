<odoo>
  <data>
    <template id="assets_frontend" inherit_id="website.assets_frontend" name="VistaCodigoPostal">
        <xpath expr="script[last()]" position="after">
          <script type="text/javascript" src="/mx_integritas_theme_integriatas/static/src/js/custom.js"></script>
          <link rel="stylesheet" href="/mx_integritas_theme_integriatas/static/src/css/custom.css"/>
          <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1" crossorigin="anonymous"/>
        </xpath>
    </template>
    <template id="mx_integritas_copyright" inherit_id="web.frontend_layout" name="Copyright Integritas mx" >
        <xpath expr="//div[@class='o_footer_copyright']" position="replace">
          <div class="o_footer_copyright">
            <div class="container py-3">
              <div class="row">
                <div class="col-sm text-center text-sm-left text-muted">
                    <t t-call="web.debug_icon"/>
                    <span>Copyright &amp;copy;</span> <span t-field="res_company.name" itemprop="name">Company name</span>
                    
                </div>
                <div class="col-sm text-center text-sm-right">
                    <span>Creado por</span> <a href="https://integritas.mx" target="_blank" class="text-white" style="text-decoration:underline">Integritas</a>
                </div>
              </div>
            </div>
          </div>
        </xpath>
    </template>
  </data>
</odoo>