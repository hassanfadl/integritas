<odoo>
  <data>
    <template id="assets_frontend" inherit_id="website.assets_frontend" name="Codigo adicional integritas">
        <xpath expr="script[last()]" position="after">
          <script type="text/javascript" src="/mx_integritas_theme_integriatas/static/src/js/custom.js"></script>
          <script type="text/javascript" src="/mx_integritas_theme_integriatas/static/src/js/widget_integritas.js"></script>
          <link rel="stylesheet" href="/mx_integritas_theme_integriatas/static/src/css/custom.css"/>
          
        </xpath>
    </template>
  
     <template id="mx_integritas_botton_flotante" inherit_id="web.frontend_layout" name="integritasFlotante">
        <!--expr="//body[@t-att-class='body_classname']"-->
        <xpath expr="//div[@id='wrapwrap']" position="inside">
          <div id="whats-integritas"  style="width:18rem;height: 25rem;background-color: #F5EBDF;border-radius: 5px 5px 5px 5px;z-index:1400!important" class="flotante-cuadro invisible-ws font-ws">
            <table style="background:#075F56;width: 100%;height: 4rem;border-radius: 5px 5px 0px 0px">
              <tr style="margin-left: 1rem;margin-top:5px">
                        <td style="width: 50px;text-align: center">
                            <div style="background-color: #FFFFFF;border-radius: 50%;width:55px;height: 55px;text-align: center;margin-left:.5rem">
                                <img src="https://integritas.mx/wp-content/uploads/2018/01/LogoA02.png"  width="30px" style="margin-top:8px;"/>
                            </div>
                        </td>
                        <td>
                              <div style="padding:1rem;color:#FFFFFF" >
                                <span >Integritas</span> <br/>
                                <small style="font-size:.7rem">Online</small>
                              </div>
                        </td>
                        <td style="text-align:right;padding:1rem">
                            <span style="color:#FFFFFF;cursor:pointer;" id="close-ws-integritas"><i class="fa fa-times" style="color:#FFFFFF"></i></span>
                        </td>
                    </tr>
              
            </table>
                <table>
                    <tr>
                        <td style="width:270px">
                            <div style="border-radius: 5px 5px 5px 5px; width:100%; background:#FFFFFF!important;padding:1rem">
                                <span style="font-size:.9rem;color:#000000!important;">Hola, ¿Te gustaría solicitar información o una cotización? Ingresa tu email y número telefónico para ponernos en contacto contigo.</span>
                            </div>
                        </td>
                    </tr>

                </table>
                <table>
                    <tr>
                        <td style="width:80px"></td>
                        <td style="width:300px">
                            <div style="border-radius: 5px 5px 5px 5px; width:100%; background:#FFFFFF!important;padding:1rem">
                                <form>
                                    <div style="margin-top:1px">
                                        <input type="text" placeholder="Nombre"  class="form-control form-control-sm" name="wsintegritasname" required="true"/>
                                    </div>
                                    <div style="margin-top:1px">
                                        <input type="email" placeholder="Email"  class="form-control form-control-sm" name="wsintegritasemail" required="true"/>
                                    </div>
                                    <div style="margin-top:1px">
                                        <input type="number" placeholder="Telefono" class="form-control form-control-sm" name="wsintegritasphone" required="true"/>
                                    </div>
                                    <div>
                                        <small class="text-danger invisible">Campos incorrectos verifica que tu información sea valida.</small>
                                    </div>
                                    <div style="text-align:right;margin-top:2px">
                                        <span  id="btn-integritas-ws-submit"  class="btn btn-sm" style="background-color:#075F56;color:#ffffff; ">Iniciar</span>
                                    </div>
                                    
                                </form>
                            </div>
                        </td>
                    </tr>
                    
                </table>
            </div>
            <!--href="https://api.whatsapp.com/message/ODJWOG3OBNQJD1"-->
            <a  id="btn-whatsapp-integritas" class="flotante href-tel" target="_blank" style="cursor:pointer;">
              <img src="https://integritas.odoo.com/mx_integritas_theme_integriatas/static/src/img/whatsapp.png" class="img-fluid pulse" alt="" width="50rem" height="50rem"/>
            </a>
        </xpath>
      </template>
  </data>
</odoo>