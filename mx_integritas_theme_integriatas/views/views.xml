<odoo>
  <data>
    <record id="whatsapp_id_boton_integritas" model="ir.ui.view">
        <field name="name">Boton Whatsapp Integritas</field>
        <field name="model" >website</field>
        <field name="inherit_id" ref="website.view_website_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[last()]" position="after">
              <page string="Boton WhatsApp">
                  <field name="text_whatsapp" placeholder="Add a description..."/>
              </page>     
            </xpath>
        </field>
    </record>
  </data>
</odoo>